{
    "definitions": "#[derive(Debug)] pub struct Reduction { pub ty: String, pub code: String } #[derive(Debug)] pub struct Rule { pub left: String, pub right: Vec<String> }",
    "rules": [
        {
            "left": "Grammar",
            "right": [
                "ProductionList"
            ],
            "reduction": {
                "ty": "Vec<Rule>",
                "code": "$$ = $1;"
            }
        },
        {
            "left": "ProductionList",
            "right": [
                "ProductionList",
                "Production"
            ],
            "reduction": {
                "ty": "Vec<Rule>",
