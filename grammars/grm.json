            "reduction": {
                "ty": "Option<String>",
                "code": "$$ = Some($1.value.clone());"
            }
        },
        {
            "left": "OptReductionAction",
            "right": [
                "ε"
            ],
            "reduction": {
                "ty": "Option<String>",
                "code": "$$ = None;"
            }
