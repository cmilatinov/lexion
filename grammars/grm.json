                "code": "$$ = vec![];"
            }
        },
        {
            "left": "NonTerminal",
            "right": [
                "'non_terminal_literal'"
            ],
            "reduction": {
                "ty": "String",
                "code": "$$ = $1.value.clone();"
            }
        },
        {
            "left": "Terminal",
            "right": [
                "'terminal_literal'"
            ],
            "reduction": {
                "ty": "String",
                "code": "$$ = $1.value.clone();"
            }
        },
        {
            "left": "Symbol",
            "right": [
                "'non_terminal_literal'"
