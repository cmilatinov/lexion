            ],
            "reduction": {
                "ty": "Vec<Vec<String>>",
                "code": "$1.push($3); $$ = $1;"
            }
        },
        {
            "left": "RuleList",
            "right": [
                "SymbolList"
            ],
            "reduction": {
                "ty": "Vec<Vec<String>>",
